#!/bin/bash
#shellcheck disable=SC2164

source ../../utils.sh

arch=(
    "linux-x64"
)

declare -A opts=(
    ["O0"]="-O0"
    ["O1"]="-O1"
    ["O2"]="-O2"
    ["O3"]="-O3"
    ["Of"]="-Ofast"
    ["Os"]="-Os"
    ["Og"]="-Og"
    ["debug"]="-g"
    ["static"]="-static"
    # ["static-pie"]="-static-pie"
)

loop_dirs make_all gcc g++
loop_dirs mv bin/* "$__BASE_DIR/bin"

unset arch
unset opts
